{
  "routes": [
    {
      "path": "/",
      "element": "web-widget",
      "properties": {
        "import": "@growing-web-examples/home-app"
      }
    },
    {
      "path": "/vue2",
      "element": "web-widget",
      "properties": {
        "import": "@growing-web-examples/vue2-app"
      }
    },
    {
      "path": "/vue-router/*",
      "element": "web-widget",
      "properties": {
        "import": "@growing-web-examples/vue-router-app"
      }
    },
    {
      "path": "/vue3",
      "element": "web-widget",
      "properties": {
        "import": "@growing-web-examples/vue3-app"
      }
    },
    {
      "path": "/react",
      "element": "web-widget",
      "properties": {
        "import": "@growing-web-examples/react-app"
      }
    },
    {
      "path": "*",
      "element": "web-widget",
      "properties": {
        "import": "@growing-web-examples/404-app"
      }
    }
  ]
}
