{
  "publicPath": "https://cdn.example.com/base/",
  "importmap": {
    "imports": {
      "systemjs": "workspace:*",
      "@growing-web/bootstrap": "workspace:*",
      "vue": "https://cdn.jsdelivr.net/npm/vue@2.6.14/dist/vue.runtime.min.js",
      "@apps/*": "workspace:*"
    }
  },
  "routemap": {
    "routes": [
      {
        "path": "/",
        "element": "web-widget",
        "properties": {
          "import": "@apps/index"
        }
      },
      {
        "path": "/vue-router-demo/*",
        "element": "web-widget",
        "properties": {
          "import": "@apps/vue-router-demo"
        }
      },
      {
        "path": "*",
        "element": "web-widget",
        "properties": {
          "import": "@apps/404"
        }
      }
    ]
  }
}