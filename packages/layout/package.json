{
  "name": "@growing-web/layout",
  "version": "0.0.4",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vue2-adapter": {
      "require": "./dist/vue2-adapter.cjs",
      "import": "./dist/vue2-adapter.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "pnpm unbuild",
    "dev": "pnpm unbuild --stub",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "unbuild": "^0.7.4"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts",
        "./dist/*",
        "./*"
      ]
    }
  }
}
