{
  "$schema": "https://unpkg.com/@growing-web/web-builder-config@latest/web-project-schema.json",
  "schemaVersion": "1.0.0",
  "manifests": ["exports-manifest"],
  "entries": [
    {
      "input": "src/index.js",
      "output": {
        "dir": "${workspaceRoot}/dist/systemjs",
        "formats": ["iife"],
        "entryFileNames": "system.[hash].js",
        "meta": {
          "umdName": "systemjs"
        }
      }
    }
  ]
}
