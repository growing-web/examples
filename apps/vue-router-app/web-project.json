{
  "$schema": "https://unpkg.com/@growing-web/web-builder-config@latest/web-project-schema.json",
  "schemaVersion": "1.0.0",
  "manifests": ["exports-manifest"],
  "entries": [
    {
      "input": "src/index.js",
      "output": {
        "dir": "dist",
        "formats": ["esm"],
        "entryFileNames": "[name].js",
        "externals": ["vue"],
        "globals": {
          "vue": "Vue"
        }
      }
    }
  ],
  "server": {
    "port": 6080
  }
}
