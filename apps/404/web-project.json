{
  "$schema": "https://unpkg.com/@growing-web/web-builder-config@latest/web-project-schema.json",
  "schemaVersion": "1.0.0",
  "manifests": ["exports-manifest"],
  "entries": [
    {
      "input": "src/main.js",
      "output": {
        "dir": "${workspaceRoot}/dist/apps/404",
        "formats": ["esm", "system"],
        "entryFileNames": "[name]-[hash]-[format].js"
      }
    }
  ],
  "server": {
    "port": 6050
  }
}
