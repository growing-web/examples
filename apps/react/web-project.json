{
  "schemaVersion": "1.0.0",
  "manifests": ["exports-manifest"],
  "entries": [
    {
      "input": "src/main.jsx",
      "output": {
        "dir": "${workspaceRoot}/dist/apps/react",
        "formats": ["esm", "system"],
        "entryFileNames": "[name]-[hash]-[format].js",
        "externals": ["vue"],
        "globals": {
          "vue": "Vue"
        }
      }
    }
  ],
  "server": {
    "port": 6070
  }
}
